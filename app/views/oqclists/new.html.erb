<%= form_for @oqclist, :url => oqclists_path do |f| %>
    <%= f.label :customer_id, "Par No:" %>
    <%= f.select :customer_id, Customer.all.map{ |x|[x.name,x.id]} %>
    <%= f.label :lotno, "Lot No:" %>
    <%= f.text_field :lotno %>
    <%= f.label :product_id, "Par No:" %>
    <%= f.select :product_id, Product.all.map{ |x|[x.partno,x.id]} %>
    <%= f.label :quantity, "Lot No:" %>
    <%= f.number_field :quantity %>
    <%= f.submit "Create" %>
<% end %>